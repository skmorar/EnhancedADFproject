{
	"name": "ds_csv_files",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_BlobSource",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"container": "adfproject"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "airline_id",
				"type": "String"
			},
			{
				"name": "airline_name",
				"type": "String"
			},
			{
				"name": "country",
				"type": "String"
			}
		]
	}
}